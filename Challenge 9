#Total sum

def product(sum):
     for a in range(1, 1001):

#a<b         
         for b in range(1, sum - a):
#b<c
#sum -a-b
             c = sum - a - b
#if correct a squared + b squared = c squared
             if a**2 + b**2 == c**2:
                print(a * b * c)

            
product(1000)
