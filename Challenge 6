# 1^2 + 2^2 + ... + 10^2 = 385

count1 = 0
for i in range(1,101):
    squared = i ** 2
    count1 = count1 + squared
print(count1)

#(1 + 2 + ... + 10)^2 = 55^2 = 3025

count2 = 0
for i in range(1,101):
    count2 = count2 + i

count2squared = count2 ** 2
print(count2squared)

total = count2squared - count1
print(total)
